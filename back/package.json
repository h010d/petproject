{
  "name": "webpacktemplite",
  "version": "1.0.0",
  "description": "webpack lite",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack --mode=development --config build/webpack.dev.conf.js",
    "start": "NODE_ENV=development webpack-dev-server --mode=development --config build/webpack.dev.conf.js",
    "dev:watch": "NODE_ENV=development webpack --mode=development --watch --config build/webpack.dev.conf.js",
    "start:open": "NODE_ENV=development webpack-dev-server --mode=development --open  --config build/webpack.dev.conf.js",
    "build": "NODE_ENV=production webpack --mode=production --config build/webpack.build.conf.js",
    "build:start": "NODE_ENV=production webpack-dev-server --mode=production --config build/webpack.build.conf.js"
  },
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acubick/webpackTemplateLite.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "acubick",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.17",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.2.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "jquery": "^3.4.1"
  }
}
